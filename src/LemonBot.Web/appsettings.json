{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AzureAdB2C": {
    "Instance": "https://{TENANT}.b2clogin.com/",
    "ClientId": "{SERVER API APP CLIENT ID}",
    "Domain": "{TENANT DOMAIN}",
    "SignUpSignInPolicyId": "{SIGN UP OR SIGN IN POLICY}"
  },
  "IdentityServer": {
    "Clients": {
      "KITT.Console": {
        "Profile": "IdentityServerSPA",
        "RedirectUri": "/console/authentication/login-callback",
        "LogoutUri": "/console/authentication/logout-callback"
      }
    }
  },
  "AllowedHosts": "*"
}
